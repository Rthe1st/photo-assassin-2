    "socket.io-client": "^4.2.0",
    "winston": "^3.3.3"
  },
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "check-format": "prettier --check .",
    "start": "node --experimental-specifier-resolution=node ./dist/server/start.js",
    "build-server": "tsc --build ./src/server/tsconfig.json",
    "build-client": "webpack --config webpack.config.ts",
    "build": "npm run build-server && npm run build-client",
    "pretest": "npm run build-server && npm run build-client",
    "integration-test": "node --experimental-vm-modules --unhandled-rejections=strict node_modules/jest/bin/jest.js --projects ./integration_tests --forceExit",
    "test": "node --experimental-vm-modules --unhandled-rejections=strict node_modules/jest/bin/jest.js",
    "build-bots": "tsc --build ./src/bots/tsconfig.json",
    "bots": "npm run build-bots && node --experimental-specifier-resolution=node ./dist/bots/start.js",
    "upload-source-maps": "sentry-cli releases --org photo-snipe --project photo-snipe files 0.0.1 upload-sourcemaps ./dist/shared ./dist/server"
  },
  "engines": {
    "node": "14.6.x",
    "npm": "6.14.x"
  },
  "devDependencies": {
    "@sentry/webpack-plugin": "^1.17.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/googlemaps": "^3.39.13",
