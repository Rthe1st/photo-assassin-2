{
  "name": "photo-assassin",
  "version": "0.0.1",
  "description": "",
  "dependencies": {
    "@google-cloud/storage": "^5.15.0",
    "@sentry/browser": "^6.13.3",
    "@sentry/node": "^6.13.3",
    "cookie-parser": "^1.4.5",
    "cross-fetch": "^3.1.4",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "image-blob-reduce": "^3.0.1",
    "image-js": "^0.33.0",
    "mathjs": "^9.5.0",
